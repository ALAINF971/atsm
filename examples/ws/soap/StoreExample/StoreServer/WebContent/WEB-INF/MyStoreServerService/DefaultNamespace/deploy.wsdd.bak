<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from MyStoreServerService WSDL service -->

  <service name="MyStoreServer" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://DefaultNamespace"/>
      <parameter name="wsdlServiceElement" value="MyStoreServerService"/>
      <parameter name="schemaQualified" value="http://DefaultNamespace"/>
      <parameter name="wsdlServicePort" value="MyStoreServer"/>
      <parameter name="className" value="DefaultNamespace.MyStoreServerSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="MyStoreServer"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="buyItems" qname="operNS:buyItems" xmlns:operNS="http://DefaultNamespace" returnQName="retNS:buyItemsReturn" xmlns:retNS="http://DefaultNamespace" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:item" xmlns:pns="http://DefaultNamespace" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:quantity" xmlns:pns="http://DefaultNamespace" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="displayContent" qname="operNS:displayContent" xmlns:operNS="http://DefaultNamespace" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="displayContent buyItems"/>

  </service>
</deployment>
